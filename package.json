    "file-icons",
    "folder-icons",
    "vscode"
  ],
  "author": "MatÄ›j Chalk <matejchalk@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matejchalk/material-file-icons.git"
  },
  "homepage": "https://github.com/matejchalk/material-file-icons#readme",
  "bugs": {
    "url": "https://github.com/matejchalk/material-file-icons/issues"
  },
