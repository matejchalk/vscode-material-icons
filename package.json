{
  "name": "vscode-material-icons",
  "version": "0.1.1",
  "license": "MIT",
  "description": "VSCode Material Icons wrapper for applications",
  "keywords": [
    "icons",
    "material-icons",
    "file-icons",
    "folder-icons",
    "vscode"
  ],
  "author": "MatÄ›j Chalk <matejchalk@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matejchalk/vscode-material-icons.git"
  },
  "homepage": "https://github.com/matejchalk/vscode-material-icons#readme",
  "bugs": {
    "url": "https://github.com/matejchalk/vscode-material-icons/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "release": "release-it",
    "fetch-icons": "node scripts/fetch-icons.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@picocss/pico": "^2.0.6",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "fs-extra": "^11.2.0",
    "globals": "^15.13.0",
    "match-sorter": "^8.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "publint": "^0.2.12",
    "simple-git": "^3.27.0",
    "svelte": "^5.7.0",
    "svelte-check": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.3",
    "vitest": "^3.0.5"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false
}
