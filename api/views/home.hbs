<div id='search-results'>
  {{#unless icons}}
    <article>
      No icons matched by search term.
    </article>
  {{/unless}}

  <div id='grid'>
    {{#each icons}}
      <article class='cell'>
        <img
          src='/icons/{{this}}.svg'
          alt='{{this}}.svg'
          width='64'
          height='64'
          loading='lazy'
        />
        <small class='name'>{{this}}</small>
      </article>
    {{/each}}
  </div>
</div>